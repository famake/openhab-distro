        if (voice.contains("game") || voice.contains("gang") || voice.contains("spain")) {
            if ((voice.contains("on") || voice.contains("paul") || voice.contains("p책")) {
                sendCommand(lifx_colorlight_d073d50352d9_power, ON) //gang 1
                sendCommand(lifx_colorlight_d073d503be21_power, ON) // gang 2
            }
            else {
                sendCommand(lifx_colorlight_d073d50352d9_power, OFF) //gang 1
                sendCommand(lifx_colorlight_d073d503be21_power, OFF) // gang 2
            }
        }
        else {
            if ((voice.contains("on") || voice.contains("paul") || voice.contains("p책")) {
                sendCommand(lifx_colorlight_d073d502f5aa_power, ON) //stue pc
            }
            else {
                sendCommand(lifx_colorlight_d073d502f5aa_power, OFF) //stue pc
            }
        }
    }
    else if ((voice.contains("projector") || voice.contains("prosjektor")) || (voice.contains("projektor") || (voice.contains("prosjekter")) {
        if ((voice.contains("on") || voice.contains("paul") || voice.contains("p책")) {
